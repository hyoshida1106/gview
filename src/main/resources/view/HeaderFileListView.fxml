<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<SplitPane fx:id="headerFileListPane" dividerPositions="0.5"
           xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
           AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="0.0"
           AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
           stylesheets="@Gview.css" fx:controller="gview.gui.commitinfo.HeaderFileListCtrl">
    <AnchorPane>
        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <Label fx:id="stagedFileLabel" text="ステージ"/>
            <TableView fx:id="stagedFileList">
                <columns>
                    <TableColumn fx:id="stagedFileTypeColumn" editable="false" reorderable="false" text="種別" minWidth="50"/>
                    <TableColumn fx:id="stagedFilePathColumn" editable="false" reorderable="false" text="パス"/>
                </columns>
            </TableView>
        </VBox>
    </AnchorPane>
    <AnchorPane>
        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <Label fx:id="changedFileLabel" text="更新"/>
            <TableView fx:id="changedFileList">
                <columns>
                    <TableColumn fx:id="changedFileTypeColumn" editable="false" reorderable="false" text="種別" minWidth="50"/>
                    <TableColumn fx:id="changedFilePathColumn" editable="false" reorderable="false" text="パス"/>
                </columns>
            </TableView>
        </VBox>
    </AnchorPane>
</SplitPane>
